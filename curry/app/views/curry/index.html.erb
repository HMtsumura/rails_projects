<h1>Curry Search</h1>
<h2><%= link_to("新規登録","/curry/new") %></h2>
<%= form_tag("/login") do %>
 <p>ユーザー名</p>
  <!-- name属性を追加してください -->
  <input name ="name">
  <p>パスワード</p>
  <!-- name属性を追加してください -->
  <input type="password" name ="password">
  <input type="submit" value="ログイン">
  <!-- form_tag用のendを追加してください -->
 <% end %>
