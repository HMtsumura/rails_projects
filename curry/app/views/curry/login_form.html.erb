<h1>ログイン画面</h1>

<%= form_tag("/login") do %>
 <p>ユーザー名</p>
  <!-- name属性を追加してください -->
  <input name ="name">
  <p>パスワード</p>
  <!-- name属性を追加してください -->
  <input type="password" name ="password">
  <input class="btn btn-primary" type="submit" value="ログイン">
  <!-- form_tag用のendを追加してください -->
 <% end %>
